{
  "name": "marzioperez/filament-media-manager",
  "description": "Media manager + fields for Filament using Spatie Media Library",
  "keywords": ["laravel", "filament", "media", "manager", "spatie"],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Marzio",
      "email": "tu-correo@example.com"
    }
  ],
  "require": {
    "php": "^8.4",
    "laravel/framework": "^11.0|^12.0",
    "filament/filament": "^4.0",
    "livewire/livewire": "^3.0",
    "spatie/laravel-medialibrary": "^11.0"
  },
  "autoload": {
    "psr-4": {
      "Marzio\\MediaManager\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Marzio\\MediaManager\\MediaManagerServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}